<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.lenght<=0){%>
                <h1>Nothing there!</h1>
                
            <%}else{%>
                <ul>
                    <% orders.forEach(order=>{%>
                        <li>
                            <h1><%= order.id %> </h1>
                        </li>
                        <ul>
                            <% order.products.forEach(product=>{%>
                            <li><%= product.title %> </li>
                            <li><%= product.orderItem.quantity %> </li>

                            <% }) %> 
                        </ul>
                    <%}) %> 
                </ul>        
            <%} %> 
        </main>
        <%- include('../includes/end.ejs') %>